{
  "expo": {
    "name": "novies",
    "slug": "novies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon.png",
    "primaryColor": "#000000",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.heesang.novies",
      "buildNumber": "1.0.0"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.BDAPPS.novies"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
